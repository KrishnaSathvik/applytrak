# ============================================================================
# ApplyTrak Example Environment File
# Copy this to .env.local and fill with your real values
# ============================================================================
REACT_APP_NAME=ApplyTrak
REACT_APP_DESCRIPTION="Track your job search journey"
REACT_APP_URL=https://applytrak.com

# ============================================================================
# SUPABASE CONFIGURATION
# ============================================================================
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# ============================================================================
# CLOUD SYNC SETTINGS
# ============================================================================
REACT_APP_CLOUD_SYNC_ENABLED=true
REACT_APP_SYNC_INTERVAL=15
REACT_APP_SYNC_BATCH_SIZE=50
REACT_APP_SYNC_COMPRESSION=true

# ============================================================================
# ANALYTICS CONFIGURATION
# ============================================================================
REACT_APP_CLOUD_ANALYTICS=true
REACT_APP_ANALYTICS_RETENTION=90

# ============================================================================
# DEVELOPMENT SETTINGS
# ============================================================================
NODE_ENV=development
REACT_APP_SUPABASE_DEBUG=true

# ============================================================================
# BACKUP SETTINGS
# ============================================================================
REACT_APP_BACKUP_RETENTION=30
REACT_APP_MAX_CLOUD_BACKUPS=10
